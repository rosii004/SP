/*
Од стандарден влез се внесуваат два цели броја N и Х.
Да се најде најблискот број помал од N коj е тотално различен од бројот Х.
Еден број е тотално различен од друг ако и само ако во него не се појавува ниту една од цифрите на другиот број.
Задачата да се реши без употреба на низи и матрици.
*/

#include <iostream>
using namespace std;
bool isTotallyDiffrent(int a, int b) {
    int tempA = a;
    int tempB;
    while (tempA > 0) {
        int digitA = tempA % 10;
        tempB = b;

        while (tempB > 0) {
            int digitB = tempB % 10;
            if (digitB == digitA)
                return false;
            tempB /= 10;
        }
            tempA/=10;
        }
        return true;
    }

int main() {
int N, X;
cin>>N>>X;
for (int i = N-1; i >= 0; i--){
    if (isTotallyDiffrent(i, X)){
        cout<<i;
        return 0;
    }
}
cout<<"Nema resenie";
    return 0;
}
