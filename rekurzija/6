#include <iostream>
using namespace std;
#define MAX 100

int findMax(int a[], int n){
    if (n == 1) return a[0];

    return max(a[n-1], findMax(a, n-1));
}

int findMin(int a[], int n){
    if (n == 1) return a[0];

    return min(a[n-1], findMin(a, n-1));
}


int main() {
    int n;
    cin>>n;
    int a[MAX];
    for(int i = 0; i < n; i++){
        cin>>a[i];
    }
    cout<<findMax(a, n)<<endl;
    cout<<findMin(a, n);
    return 0;
}
