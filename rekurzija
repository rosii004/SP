
#include <iostream>
using namespace std;
#define MAX 100

int countOccurances(int a[], int n, int element){
    if (n == 0) return 0;

    if (a[0] == element)
        return 1 + countOccurances(a + 1, n - 1, element);
    else
        return countOccurances(a + 1, n - 1, element);
}

int main() {
    int n, x;
    cin>>n;

    int a[MAX];

    for (int i = 0; i < n; ++i) {
        cin>>a[i];
    }

    cin>>x;

    cout<<countOccurances(a, n, x);

    return 0;
}
