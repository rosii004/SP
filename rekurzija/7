#include <iostream>
using namespace std;

int multiply(int a, int b) {
    if (b == 0) return 0;
    if (b > 0) return a + multiply(a, b - 1);
    return -multiply(a, -b);
}

int main() {
    int a, b;
    cout << "Enter two numbers: ";
    cin >> a >> b;

    cout << a << " * " << b << " = " << multiply(a, b) << endl;

    return 0;
}
