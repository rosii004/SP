#include <iostream>
using namespace std;

void permute(string &s, int l, int r) {
    if (l == r) {
        cout << s << endl;
        return;
    }
    for (int i = l; i <= r; i++) {
        swap(s[l], s[i]); 
        permute(s, l + 1, r);
        swap(s[l], s[i]);
    }
}

int main() {
    string str;
    cout << "Enter a string: ";
    cin >> str;

    cout << "All permutations of " << str << " are:" << endl;
    permute(str, 0, str.length() - 1);

    return 0;
}
