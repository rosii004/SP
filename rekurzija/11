#include <iostream>
using namespace std;
#define MAX 100

int sumArray(int a[], int n){
    if (n == 0) return 0;
    return a[n-1] + sumArray(a, n-1);
}

int main() {
    int n;
    cin>>n;
    int a[MAX];
    for (int i = 0; i < n; ++i) {
        cin>>a[i];
    }
    cout<<sumArray(a, n);

    return 0;
}
